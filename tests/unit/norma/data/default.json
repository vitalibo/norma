[
  {
    "description": "fill missing values with default",
    "engines": [
      "pandas",
      "pyspark"
    ],
    "given": {
      "data": [
        {
          "name": "John",
          "age": 30
        },
        {
          "name": "Nancy"
        }
      ]
    },
    "when": {
      "json_schema": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "age": {
            "type": "integer",
            "default": 25
          }
        }
      }
    },
    "then": {
      "data": [
        {
          "name": "John",
          "age": 30,
          "errors": {}
        },
        {
          "name": "Nancy",
          "age": 25,
          "errors": {}
        }
      ]
    }
  },
  {
