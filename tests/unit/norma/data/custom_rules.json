[
  {
    "description": "() -> $.col.startswith('f'), error is raised [vectorized]",
    "engines": [
      "pandas"
    ],
    "given": {
      "data": [
        {
          "col": "foo"
        },
        {
          "col": "bar"
        }
      ]
    },
    "when": {
      "schema": {
        "columns": {
          "col": {
            "dtype": "str",
            "rules": {
              "expr": "rules.rule(lambda df, column: ~df[column].str.startswith('f'), type='custom_error', msg='column must start with f')"
            }
          }
        }
      }
    },
    "then": {
      "data": [
        {
          "col": "foo",
          "errors": {}
        },
        {
          "col": null,
          "errors": {
            "col": {
              "details": [
                {
                  "msg": "column must start with f",
                  "type": "custom_error"
                }
              ],
              "original": "\"bar\""
            }
          }
        }
      ]
    }
  },
